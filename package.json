{
  "name": "social-racetrack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "clsx": "^1.1.0",
    "firebase": "^7.14.4",
    "firebase-tools": "^8.4.0",
    "jquery": "^3.5.0",
    "material-ui-dropzone": "^3.3.0",
    "mdbreact": "^4.25.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-avatar": "^3.9.7",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.5.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-toastify": "^6.0.5",
    "uuid": "^8.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "fb:db": "firebase deploy --only firestore:rules",
    "fb:stor": "firebase deploy --only storage:rules",
    "fb:fun": "firebase deploy --only functions",
    "fb:host": "yarn build && firebase deploy --only hosting"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended"
    ],
    "rules": {
      "prefer-const": "error",
      "no-var": "error",
      "no-extra-boolean-cast": "off",
      "max-len": [
        "error",
        {
          "code": 105
        }
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "eol-last": [
        "error",
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
